{
    "name": "lacernest/laravel-cart",
    "description": "Laravel Cart is designed to be highly customizable and flexible, making it easy to adapt to your specific use case.",
    "keywords": [
        "lacernest",
        "laravel",
        "framework",
        "cart",
        "laravel-cart",
        "laravel cart",
        "laravel cart package",
        "laravel package",
        "laravel-cart-shop",
        "laravel-shop",
        "laravel e-commerce",
        "laravel basket"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "homepage": "https://github.com/lacernest/laravel-cart",
    "authors": [
        {
            "name": "LacErnest",
            "email": "ernestsamo16@gmail.com",
            "role": "author",
            "homepage": "https://github.com/LacErnest"
        }
    ],
    "autoload": {
        "psr-4": {
            "LacErnest\\LaravelCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "^1.4.0|^2.0.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html ./coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LacErnest\\LaravelCart\\Providers\\LaravelCartServiceProvider"
            ]
        }
    }
}
